{
  "name": "@cloudflare/agents-repo",
  "description": "The monorepo for Cloudflare Agents and related work",
  "type": "module",
  "keywords": [
    "cloudflare",
    "workers",
    "ai",
    "agents",
    "durable objects",
    "workflows"
  ],
  "author": "Sunil Pai <spai@cloudflare.com>",
  "private": true,
  "scripts": {
    "build": "npm run build -w agents -w hono-agents -w @cloudflare/ai-chat -w @cloudflare/codemode",
    "format": "oxfmt --write .",
    "format:check": "oxfmt --check .",
    "lint": "oxlint examples/ packages/ guides/ openai-sdk/ site/ai-playground/",
    "typecheck": "npx tsx scripts/typecheck.ts",
    "check:exports": "npx tsx scripts/check-exports.ts",
    "check:updates": "npx tsx scripts/check-updates.ts",
    "check": "sherif && npm run check:exports && oxfmt --check . && oxlint examples/ packages/ guides/ openai-sdk/ site/ai-playground/ && npm run typecheck",
    "test": "npm run test -w agents -w @cloudflare/ai-chat",
    "test:react": "npm run test:react -w agents",
    "postinstall": "patch-package && playwright install --with-deps chromium",
    "prepare": "husky"
  },
  "license": "MIT",
  "version": "0.0.0",
  "devDependencies": {
    "@ai-sdk/anthropic": "^3.0.38",
    "@ai-sdk/google": "^3.0.22",
    "@ai-sdk/openai": "^3.0.26",
    "@ai-sdk/react": "^3.0.79",
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@cloudflare/kumo": "^1.1.0",
    "@cloudflare/vite-plugin": "^1.23.1",
    "@cloudflare/vitest-pool-workers": "^0.12.10",
    "@cloudflare/workers-types": "^4.20260207.0",
    "@modelcontextprotocol/sdk": "1.26.0",
    "@openai/agents": "^0.4.6",
    "@openai/agents-extensions": "^0.4.6",
    "@phosphor-icons/react": "^2.1.10",
    "@types/node": "^25.2.2",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.3",
    "@vitest/browser": "^3.2.4",
    "@vitest/runner": "^3.2.4",
    "ai": "^6.0.77",
    "autoevals": "^0.0.131",
    "concurrently": "^9.2.1",
    "evalite": "^0.19.0",
    "fast-glob": "^3.3.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "oxfmt": "^0.28.0",
    "oxlint": "^1.43.0",
    "partyserver": "^0.1.3",
    "partysocket": "1.1.12",
    "patch-package": "^8.0.1",
    "pkg-pr-new": "^0.0.63",
    "playwright": "^1.58.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "sherif": "^1.10.0",
    "tsdown": "^0.20.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-devtools-json": "^1.0.0",
    "vitest": "3.2.4",
    "vitest-browser-react": "^1.0.1",
    "workers-ai-provider": "^3.0.4",
    "wrangler": "^4.63.0",
    "zod": "^4.3.6"
  },
  "workspaces": [
    "examples/*",
    "packages/*",
    "guides/*",
    "openai-sdk/*",
    "site/*"
  ],
  "overrides": {
    "esbuild": "^0.25.1"
  },
  "lint-staged": {
    "*": [
      "oxfmt --write"
    ]
  },
  "packageManager": "npm@11.9.0"
}
