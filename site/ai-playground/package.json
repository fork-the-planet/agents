{
  "name": "@cloudflare/agents-ai-playground",
  "type": "module",
  "private": true,
  "author": "AI Agents Team <1800-agents@cloudflare.com>",
  "scripts": {
    "start": "vite dev",
    "clean": "rm -rf .wrangler node_modules/.vite dist",
    "build": "npm run clean && vite build",
    "deploy": "npm run build && wrangler deploy --var HOST:https://playground.ai.cloudflare.com --domain playground.ai.cloudflare.com",
    "types": "wrangler types env.d.ts --include-runtime false --strict-vars false",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.0",
    "@ai-sdk/google": "^3.0.0",
    "@ai-sdk/openai": "^3.0.0",
    "ai": "^6.0.0",
    "ai-gateway-provider": "https://pkg.pr.new/cloudflare/ai/ai-gateway-provider@338",
    "downshift": "^9.0.13",
    "nanoid": "^5.1.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hotkeys-hook": "^5.2.1",
    "react-textarea-autosize": "^8.5.9",
    "streamdown": "^1.6.10",
    "workers-ai-provider": "https://pkg.pr.new/cloudflare/ai/workers-ai-provider@338",
    "zod": "^4.1.8"
  },
  "keywords": []
}
